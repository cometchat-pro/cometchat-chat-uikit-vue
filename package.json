{
  "name": "@cometchat/chat-uikit-vue",
  "version": "4.3.3",
  "private": false,
  "description": "CometChat UI Kit for VueJS",
  "main": "dist/index.umd.js",
  "module": "dist/index.js",
  "license": "https://www.cometchat.com/legal-terms-of-service",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "release": "npm run build && npm publish"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "core-js": "^3.8.3",
    "vite-plugin-dts": "^2.3.0",
    "vue": "^3.3.2"
  },
  "peerDependencies": {
    "@cometchat/chat-sdk-javascript": "^4.0.4",
    "@cometchat/uikit-elements": "^4.3.4",
    "@cometchat/uikit-resources": "^4.3.3",
    "@cometchat/uikit-shared": "^4.3.4"
    },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.2.0",
    "@tsconfig/node18": "^2.0.1",
    "@types/node": "^18.16.8",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vite-aliases": "^0.11.1",
    "vue-tsc": "^1.6.4"
  }
}
